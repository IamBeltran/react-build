    "build-react": "webpack --config wp.prd.config.js",
    "build-server": "json-server -c ./jsonserver/json-server.json ./jsonserver/json-server-data.json",
    "jsonserver": "json-server -c ./jsonserver/json-server.json ./jsonserver/json-server-data.json",
    "concurrently": "set NODE_ENV=production && node concurrently.js",
